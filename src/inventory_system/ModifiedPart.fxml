                     <font>
                        <Font name="Mongolian Baiti" size="14.0" />
                     </font>
                  </Button>
                  <Button maxWidth="75.0" mnemonicParsing="false" onAction="#pushCancel" prefWidth="75.0" text="Cancel">
                     <font>
                        <Font name="Mongolian Baiti" size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
