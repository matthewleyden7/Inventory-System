                        </Label>
                        <TextField fx:id="companyField" maxWidth="150.0" prefWidth="150.0" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="Mongolian Baiti" size="14.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
               </children>
            </VBox>
