                        <TextField fx:id="maxField" maxWidth="82.0" prefHeight="26.0" prefWidth="82.0" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="Mongolian Baiti" size="14.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="20.0" right="20.0" />
                           </HBox.margin>
                        </TextField>
                        <Label prefHeight="16.0" prefWidth="35.0" text="Min">
                           <font>
                              <Font name="Mongolian Baiti" size="14.0" />
